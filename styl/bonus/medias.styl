@media screen and (min-width 2295px)
	body
		font-size: 20px
	.container
		width 1400px
	#feedback
		min-height: calc(100vh - 796px);
	#footer p
		font-size: 16px
	#main.remind
		min-height calc(100vh - 210px)
	.remind_main_title 
		margin 160px 0 40px
@media screen and (max-width 1199px)
	#logo
		width 220px
		float: none
		display block
		margin 0 auto 10px
	.contacts
		float none
		text-align: center
	.contact
		margin 0 5px
@media screen and (max-width 991px)
	.contact
		margin 5px
	#main
		padding: 0 0 50px
	.pre
		margin-top: 0
	#feedback
		padding 40px 0
	#footer
		padding 20px 0
@media screen and (max-width 767px)
	#vk
		margin-bottom: 25px
@media screen and (max-width 485px)
	#header
		padding: 15px 0
	#logo
		width 160px
	.contact
		font-size: 14px
		line-height: 18px
		background-size 18px
		padding-left: 22px
	.main_title
		font-size: 26px
	.general_title
		font-size: 22px
	#footer p
		font-size: 12px
	.main_subtitle
		font-size: 20px
	.remind_main_subtitle 
		margin-top: 35px
@media screen and (max-width 380px)
	#main
		padding 0 0 30px
	.main_title
		font-size: 24px
	#feedback
		padding 30px 0
	.general_title
		font-size: 20px
	.soc_buttons a
		margin 15px 5px 10px
	.remind_main_subtitle 
		margin 25px 0 0
		font-size: 18px